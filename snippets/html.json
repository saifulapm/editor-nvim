{
  "!-": {
    "prefix": "!-",
    "body": "<!-- ${1} -->"
  },
  "html": {
    "prefix": "html",
    "body": "<html>\n\t${1}\n</html>"
  },
  "body": {
    "prefix": "body",
    "body": "<body>\n\t${1}\n</body>"
  },
  "head": {
    "prefix": "head",
    "body": "<head>\n\t<title>${1:Page Title}</title>\n\n\t${2}\n</head>"
  },
  "title": {
    "prefix": "title",
    "body": "<title>${1:Page Title}</title>"
  },
  "script": {
    "prefix": "script",
    "body": "<script type=\"text/javascript\">\n\t${1}\n</script>"
  },
  "scripts": {
    "prefix": "scripts",
    "body": "<script src=\"${1}.js\" type=\"text/javascript\"></script>"
  },
  "style": {
    "prefix": "style",
    "body": "<style type=\"text/css\" media=\"${1:screen}\">\n\t${2}\n</style>${3}"
  },
  "styles": {
    "prefix": "styles",
    "body": "<link rel=\"${1:stylesheet}\" href=\"${2:/css/master.css}\" type=\"text/css\" media=\"${3:screen}\" />"
  },
  "br": {
    "prefix": "br",
    "body": "<br>"
  },
  "hr": {
    "prefix": "hr",
    "body": "<hr>"
  },
  "div": {
    "prefix": "div",
    "body": "<div class=\"${1:name}\">\n\t${2}\n</div>"
  },
  "clear": {
    "prefix": "clear",
    "body": "<div class=\"clear\"></div>"
  },
  "fieldset": {
    "prefix": "fieldset",
    "body": "<fieldset id=\"$1\">\n\t<legend>${1:name}</legend>\n\n\t${3}\n</fieldset>"
  },
  "form": {
    "prefix": "form",
    "body": "<form action=\"${1:/action/}\" method=\"${2:POST}\" accept-charset=\"utf-8\">\n\t${3}\n\n\t<p><input type=\"submit\" value=\"Submit\" /></p>\n</form>"
  },
  "h1": {
    "prefix": "h1",
    "body": "<h1>${1}</h1>"
  },
  "h2": {
    "prefix": "h2",
    "body": "<h2>${1}</h2>"
  },
  "h3": {
    "prefix": "h3",
    "body": "<h3>${1}</h3>"
  },
  "h4": {
    "prefix": "h4",
    "body": "<h4>${1}</h4>"
  },
  "h5": {
    "prefix": "h5",
    "body": "<h5>${1}</h5>"
  },
  "input": {
    "prefix": "input",
    "body": "<input type=\"${1:text}\" name=\"${2:some_name}\" value=\"${3}\" />${4}"
  },
  "label": {
    "prefix": "label",
    "body": "<label for=\"${2:$1}\">${1:name}</label>\n<input type=\"${3:text}\" name=\"${4:$2}\" value=\"${5}\" id=\"${6:$2}\" />"
  },
  "link": {
    "prefix": "link",
    "body": "<link rel=\"${1:stylesheet}\" href=\"${2:/css/master.css}\" type=\"text/css\" media=\"${3:screen}\" />"
  },
  "meta": {
    "prefix": "meta",
    "body": "<meta name=\"${1:name}\" content=\"${2:content}\" />${3}"
  },
  "utf": {
    "prefix": "utf",
    "body": "<meta charset=\"utf-8\" />"
  },
  "utf8": {
    "prefix": "utf8",
    "body": "<meta charset=\"utf-8\" />"
  },
  "opt": {
    "prefix": "opt",
    "body": "<option value=\"${1:option}\">${2:$1}</option>${3}"
  },
  "select": {
    "prefix": "select",
    "body": "<select name=\"${1:some_name}\" id=\"${2:$1}\">\n\t<option value=\"${3:option}\">${4:$3}</option>\n</select>${5}"
  },
  "a": {
    "prefix": "a",
    "body": "<a href=\"${2:#}\">${1:Label}</a>"
  },
  "ul": {
    "prefix": "ul",
    "body": "<ul>\n\t<li>${1}</li>\n</ul>"
  },
  "li": {
    "prefix": "li",
    "body": "<li>${1}</li>"
  },
  "table": {
    "prefix": "table",
    "body": "<table border=\"${1:0}\">\n\t<tr><th>${2:Header}</th></tr>\n\t<tr><th>${3:Data}</th></tr>\n</table>${4}"
  },
  "textarea": {
    "prefix": "textarea",
    "body": "<textarea name=\"${1:Name}\" rows=\"${2:8}\" cols=\"${3:40}\">${4}</textarea>${5}"
  },
  "s": {
    "prefix": "s",
    "body": "<${1:span}${2}>${3}</$1>"
  },
  "p": {
    "prefix": "p",
    "body": "<${1:p}${2}>\n\t${3}\n</$1>"
  }
}